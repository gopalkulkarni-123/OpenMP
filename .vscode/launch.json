{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug C/C++ Program",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/integration_multithread_dynamic_blockwise_reference",
            "args": [], // Add any command line arguments here
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build",
            "miDebuggerPath": "/usr/bin/gdb",
            "logging": {
                "trace": true,
                "traceResponse": true,
                "engineLogging": true
            }
        }
    ]
}